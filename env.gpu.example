# GPU Acceleration Configuration (DEV ONLY)
# Copy this to your .env file to enable GPU acceleration
# Requires: pip install cupy-cuda12x (or appropriate CUDA version)

# Enable/disable GPU acceleration
USE_GPU_ACCELERATION=true

# Optional: Force specific GPU device (if you have multiple GPUs)
# CUDA_VISIBLE_DEVICES=0

# Optional: GPU memory fraction (0.0-1.0, default: auto)
# CUPY_GPU_MEMORY_LIMIT=80%

# Other dev settings
ENV=development
DEBUG=true
LOG_LEVEL=INFO

